      fx:controller="slava9096.saws.sawsupdater.SawUpdaterController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="start"/>
    <Button text="Start!" onAction="#onStartButtonClick"/>
</VBox>
